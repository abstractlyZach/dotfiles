# Basic binds
super + Return
	$TERMINAL
super + d
	dmenu_run
# open file manager
super + r
	$TERMINAL -e $FILE
super + i
	$TERMINAL -e htop
super + w
	$BROWSER

# super + backtick (`)
super + grave
	dmenu_emoji

super + shift + x
	prompt "Shutdown computer?" "sudo -A shutdown -h now"
super + shift + BackSpace
	prompt "Reboot computer?" "sudo -A reboot"

# wifi connection utility
super + shift + w
	$TERMINAL -e sudo nmtui
